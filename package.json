{
  "name": "insomnia-importers",
  "version": "0.2.0",
  "author": "Gregory Schier <gschier1990@gmail.com>",
  "description": "Various data importers for Insomnia REST Client",
  "license": "MIT",
  "bin": {
    "insomnia-import": "./bin/cli"
  },
  "main": "./src/import.js",
  "scripts": {
    "test": "jest --silent && cat ./coverage/lcov.info | coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getinsomnia/importers.git"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "shell-quote": "^1.6.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "jest": "^17.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branched": 100,
        "functions": 100,
        "lines": 100,
        "statement": 100
      }
    }
  },
  "engines": {
    "node": ">=6"
  }
}
